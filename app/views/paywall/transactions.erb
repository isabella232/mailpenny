<div class="upper_nav">
  <a id="sett" href="settings">Settings</a>
  <a id="trans" href="transactions">Transactions</a>
  <a id="logout" href="logout">Logout</a>
</div>
<div class="tble_trans">
  <table class="table table-hover">
    <thead>
    <tr>
      <th>Time</th>
      <th>Sender</th>
      <th>Status</th>
    </tr>
    </thead>
    <tbody>
   <% @user.transactions.all.each do |t|%>
    <tr>
      <td><%=t.updated_at%></td>
      <td><%=t.from%></td>
      <td><%=t.status%></td>
    </tr>
    <% end%>
    </tbody>
  </table>
</div>